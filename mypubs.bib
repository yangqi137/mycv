issn = {2469-9950},
journal = {Phys. Rev. B},
month = {jul},
number = {4},
pages = {041119(R)},
title = {{Self-learning quantum Monte Carlo method in interacting fermion systems}},
url = {http://dx.doi.org/10.1103/PhysRevB.96.041119 http://link.aps.org/doi/10.1103/PhysRevB.96.041119},
volume = {96},
year = {2017}
}
