author = {Qi, Yang and Cheng, Meng and Fang, Chen},
eprint = {1509.02927},
month = {sep},
title = {{Symmetry fractionalization of visons in $\mathbb Z_2$ spin liquids}},
url = {http://arxiv.org/abs/1509.02927}
}
@article{SLMCF2017,
abstract = {We develop the self-learning Monte Carlo (SLMC) method, a general-purpose numerical method recently introduced to simulate many-body systems, for studying interacting fermion systems. Our method uses a highly-efficient update algorithm, which we design and dub "cumulative update", to generate new candidate configurations in the Markov chain based on a self-learned bosonic effective model. From general analysis and numerical study of the double exchange model as an example, we find the SLMC with cumulative update drastically reduces the computational cost of the simulation, while remaining statistically exact. Remarkably, its computational complexity is far less than the conventional algorithm with local updates.},
author = {Liu, Junwei and Shen, Huitao and Qi, Yang and Meng, Zi Yang and Fu, Liang},
